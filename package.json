{
  "name": "date-test-helpers",
  "version": "1.0.0",
  "description": "Create Date objects out of strings represented in common American timezones.",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf build/ && babel src -d build && cp package.json build/",
    "postpublish": "npm cache clean",
    "lint": "eslint .",
    "test:mocha": "NODE_ENV=test mocha --compilers=js:babel-register --require=babel-polyfill",
    "test": "npm run lint && flow && npm run test:mocha"
  },
  "keywords": [],
  "author": "Good Eggs Inc.",
  "contributors": [
    "Max Edmands <max@goodeggs.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-register": "^6.18.0",
    "eslint": "^3.12.2",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-goodeggs": "^3.3.2",
    "eslint-plugin-lodash": "^1.9.4",
    "eslint-plugin-mocha": "^4.7.0",
    "flow-bin": "^0.37.4",
    "lodash": "^4.17.2",
    "mocha": "^3.2.0"
  },
  "dependencies": {
    "moment-timezone": "^0.5.10"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016",
      "es2017"
    ],
    "plugins": [
      "transform-flow-strip-types",
      "add-module-exports"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "goodeggs",
      "flowtype"
    ],
    "extends": [
      "plugin:goodeggs/goodeggs",
      "plugin:flowtype/recommended"
    ],
    "rules": {
      "global-require": "off"
    }
  }
}
